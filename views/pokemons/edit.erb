<form class="" action="/pokemons/<%= @pokemon.id %>" method="post">
  <input type="hidden" name="_method" value="put">
  <label for="pokemon[name">Name</label>
  <input type="text" name="pokemon[name]" value="<%= @pokemon.name %>">
  <label for="pokemon[cp]">Combat Points</label>
  <input type="text" name="pokemon[cp]" value="<%= @pokemon.cp %>">
  <label for="pokemon[poke_type]">Type</label>
  <input type="text" name="pokemon[poke_type]" value="<%= @pokemon.poke_type %>">
  <label for="pokemon[img_url]">Image URL</label>
  <input type="text" name="pokemon[img_url]" value="<%= @pokemon.img_url %>">
  <input type="submit" value="Save">
</form>
